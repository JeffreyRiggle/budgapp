{
  "name": "budgapp",
  "description": "electron application for managing budget",
  "author": "Jeffrey Riggle <jeffreyriggle@gmail.com>",
  "version": "0.7.0",
  "homepage": "./",
  "main": "src/native/main.js",
  "build": {
    "extends": null,
    "appId": "com.ilusr.budgapp",
    "productName": "Budgapp",
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Productivity"
    },
    "directories": {
      "buildResources": "assets"
    }
  },
  "scripts": {
    "start": "concurrently \"yarn run react-start\" \"wait-on http://localhost:3000 && cross-env NODE_ENV=dev yarn run electron-start\"",
    "prod": "yarn run build:web && yarn run electron-start",
    "build:web": "react-scripts build",
    "build:electron": "electron-builder",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --watchAll=false",
    "test:e2e": "jest --config ./e2e.config.js --runInBand",
    "eject": "react-scripts eject",
    "react-start": "react-scripts start",
    "electron-start": "electron ."
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@jeffriggle/ipc-bridge-client": "^1.0.10",
    "@jeffriggle/ipc-bridge-server": "^1.0.6",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.2",
    "@types/react": "^17.0.37",
    "@types/react-datepicker": "^4.3.3",
    "@types/react-dom": "^17.0.11",
    "chart.js": "^3.7.0",
    "cross-env": "^7.0.2",
    "moment": "^2.24.0",
    "node-sass": "^4.11.0",
    "react": "^16.8.0",
    "react-chartjs-2": "^4.0.0",
    "react-datepicker": "^2.1.0",
    "react-dom": "^16.8.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "typescript": "^4.5.4",
    "xlsx": "^0.17.2"
  },
  "devDependencies": {
    "@testing-library/react": "^10.4.9",
    "@types/react-router-dom": "^5.3.2",
    "chromedriver": "^85.0.0",
    "concurrently": "^4.1.0",
    "electron": "^10.0.0",
    "electron-builder": "^22.8.0",
    "spectron": "^12.0.0",
    "wait-on": "^3.2.0"
  }
}
