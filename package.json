{
  "name": "budgapp",
  "version": "0.3.0",
  "homepage": "./",
  "main": "src/native/main.js",
  "build": {
    "extends": null,
    "appId": "com.ilusr.budgapp",
    "productName": "Budgapp",
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Productivity"
    }
  },
  "scripts": {
    "start": "concurrently \"yarn run react-start\" \"wait-on http://localhost:3000 && cross-env NODE_ENV=dev yarn run electron-start\"",
    "prod": "yarn run build:web && yarn run electron-start",
    "build:web": "react-scripts build",
    "build:electron": "electron-builder",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --watchAll=false && yarn run test:native",
    "test:native": "mocha ./src/native/spec",
    "test:e2e": "mocha ./test --timeout 120000",
    "eject": "react-scripts eject",
    "react-start": "react-scripts start",
    "electron-start": "electron ."
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@jeffriggle/ipc-bridge-client": "^1.0.10",
    "@jeffriggle/ipc-bridge-server": "^1.0.6",
    "chart.js": "^1.1.1",
    "cross-env": "^7.0.2",
    "moment": "^2.24.0",
    "node-sass": "^4.11.0",
    "react": "^16.8.0",
    "react-chartjs": "^1.2.0",
    "react-datepicker": "^2.1.0",
    "react-dom": "^16.8.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3"
  },
  "devDependencies": {
    "@testing-library/react": "^10.4.9",
    "chai": "^4.2.0",
    "chromedriver": "^85.0.0",
    "concurrently": "^4.1.0",
    "electron": "^9.2.1",
    "electron-builder": "^22.8.0",
    "mocha": "^8.1.1",
    "mock-require": "^3.0.3",
    "spectron": "^11.1.0",
    "wait-on": "^3.2.0"
  }
}
